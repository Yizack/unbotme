{
  "name": "unbotme",
  "version": "1.0.0",
  "description": "Bot for Twitch designed to automatically ban identified bots that persistently lurk in channels.",
  "main": "src/unbotme.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx src/unbotme.ts",
    "start": "node build/unbotme.mjs",
    "start:pm2": "pm2 start build/unbotme.mjs",
    "build": "unbuild",
    "tunnel": "cloudflared tunnel run unbotme",
    "upload": "tsx scripts/uploadToServer.ts",
    "botslist": "tsx scripts/generateBotsList.ts",
    "test": "vitest run --reporter=verbose",
    "lint": "eslint --fix"
  },
  "keywords": [],
  "author": "Yizack Rangel",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@twurple/auth": "^7.2.1",
    "@twurple/auth-tmi": "^7.2.1",
    "@types/node": "^22.10.2",
    "@types/ssh2": "^1.15.1",
    "@typescript-eslint/parser": "^8.19.0",
    "consola": "^3.3.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "ofetch": "^1.4.1",
    "socket.io": "^4.8.1",
    "ssh2": "^1.16.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^3.2.0"
  },
  "packageManager": "pnpm@9.15.2"
